{
  "name": "@raypx/database",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./envs": "./src/envs.ts",
    "./schemas": "./src/schemas/pg/index.ts",
    "./vectorSchemas": "./src/schemas/vector/index.ts",
    "./types": "./src/types.ts"
  },
  "scripts": {
    "clean": "rimraf dist .turbo",
    "generate": "rr drizzle-kit generate --config=config/drizzle.config.ts",
    "generate:vector": "rr drizzle-kit generate --config=config/drizzle-vector.config.ts",
    "migrate": "rr drizzle-kit migrate --config=config/drizzle.config.ts",
    "migrate:vector": "rr drizzle-kit migrate --config=config/drizzle-vector.config.ts",
    "push": "rr drizzle-kit push --config=config/drizzle.config.ts",
    "push:vector": "rr drizzle-kit push --config=config/drizzle-vector.config.ts",
    "seed": "rr tsx seed.ts",
    "studio": "rr drizzle-kit studio --config=config/drizzle.config.ts",
    "studio:vector": "rr drizzle-kit studio --config=config/drizzle-vector.config.ts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@raypx/config": "workspace:*",
    "@raypx/shared": "workspace:*",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "nanoid": "^5.1.6",
    "postgres": "^3.4.8",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@raypx/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "drizzle-kit": "^0.31.9",
    "drizzle-seed": "^0.3.1",
    "jiti": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
