{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Vite Application",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "types": ["vite/client"],
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "useDefineForClassFields": true,
    "verbatimModuleSyntax": false,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
